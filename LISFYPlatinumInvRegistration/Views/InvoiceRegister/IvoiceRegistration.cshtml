        $("#invPrint").on('click', function () {
            event.preventDefault();
            var invNo = $("#invNo").val();
            var corpKey = $("#corpKey").val();
            var cpyid = $("#cpyid").val();
            var yrid = $("#yrid").val();

            var PatPrntObj = {
                Inv_No: invNo,
                Inv_InsId: corpKey,
                Inv_CpyId: cpyid,
                Inv_YrId: yrid
            }
            $.ajax({
                url: "/InvoiceRegister/PatientPrint",
                type: "POST",
                data: JSON.stringify(PatPrntObj),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data) {
                    if (data.Result === "OK") {
                        window.location = "/InvoiceRegister/PDFPatientPrint";
                    }
                    else {

                    }
                }
            })
        })
        //$(".outerDiv").on('click', function () {
        //    alert(e.target.className);
        //})
        //$(document).ready(function () {
        //    $('.outerDiv').click(function (e) {
        //        alert(e.target.className);
        //    });
        //});
        $(".zoombtn").on('click', function (e) {
            var btnId = $(this).find(".fa-search").attr("id");
            var imgId = $(this).parent().find("img").attr("id");
            //var imgId = $(this).parentsUntil("#a").attr("id");
            //var imgIsd = $(this).find(".lazy").attr("id");
            var scanImg;
            if (btnId == "SrchScan1") {
                scanImg = 1;
            }
            else if (btnId == "SrchScan2") {
                scanImg = 2;
            }
            else if (btnId == "SrchScan3") {
                scanImg = 3;
            }
            else if (btnId == "SrchScan4") {
                scanImg = 4;
            }
            $.ajax({
                url: "/InvoiceRegister/ScanImage",
                type: "POST",
                data: JSON.stringify({ scanImage: scanImg }),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data) {
                    if (data.Result === "OK") {
                        var x = data.Record;
                        $(imgId).attr("src", x);
                        //alert("Order Request(s) saved");
                        //window.location = "/InvoiceRegister/IvoiceRegistration";
                    }
                    else if (data.Result === "NoScanner") {
                        alert("No Scanner Available!");
                    }

                }

            })

        })
    </script>
