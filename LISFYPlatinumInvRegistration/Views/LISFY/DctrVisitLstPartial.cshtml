<div class="modal-dialog modal-lg textEditor " style="">

    <div class="modal-content box inner-outline" style="width:450px;margin-left:300px;margin-top:80px;border-color: #808080;">
        @*<div class="modal-header" style="background-color:#85b9e0;margin-top:8px;margin-left:30px;">

                <h6 class="modal-title" style="margin-top:8px;margin-left:30px;background-color:#85b9e0;color:black">Daily Collection Statement</h6>
            </div>*@
        <div class="modal-body modal-popup " style="height:230px;width:450px;">

            <button type="button" class="close" id="DctrVisitClose" data-dismiss="modal" aria-label="Close">
                <i class="fas fa-times"></i>
            </button>
            <div class="row justify-content-center buttons-header" style="border:1px solid #808080;width:406px;margin-left:5px;height:60px;background-color:#357EC7">


                <div class="col-lg-10 col-md-4 col-sm-4">
                    <div class="row" style="margin-top:20px">
                        @*<div class="col-1" style="margin-left:95px"><label>Branch</label></div>*@
                        <div class="col-8" style="margin-right:0px">
                            @*<input name="passport" id="passport" type="text" class="form-control" style="width:350px;height:25px">*@
                            <h5 style="color:white;width:350px">No Doctor Visit</h5>
                        </div>
                    </div>
                </div>

                <div class="col-lg-2 col-md-3" style="margin-top:20px;font-family:Tahoma,Arial;margin-right:0px;">
                    <button type="button" class="green-btn" value="Refresh" id="btnDctrVisit" style="height:27px;width:60px;margin-right:10px;font-family:Verdana;font-size:13px">
                        @*<label style="font-family:Verdana;font-size:13px;font-weight:bold;align-content:center">*@Print@*</label>*@
                    </button>

                </div>

            </div>
            <div class="row justify-content-center buttons-header " style="width:456px;margin-left:5px;height:40px">
                <div class="col-lg-12 col-md-4 col-sm-4">
                    <div class="row" style="margin-top:0px">
                        <div class="col-1" style="margin-top:5px"><label>From</label></div>
                        <div class="col-5" style="margin-right:0px">
                            <input name="dctrVstFrm" id="dctrVstFrm" style="cursor :default;height:25px;width:100px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px" class="form-control" value="@ViewBag.FROMDATE" readonly />

                            <input type="" style="display:none;margin-top:10px" id="dctrVstFrmClndr" alt="tests" />
                        </div>
                        <div class="col-1" style="margin-top:5px"><label>To</label></div>
                        <div class="col-5" style="margin-right:0px">
                            <input name="dctrVstTo" id="dctrVstTo" style="cursor :default;height:25px;width:100px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px" class="form-control" value="@ViewBag.TODATE" readonly />

                            <input type="" style="display:none;margin-top:10px" id="dctrVstToClndr" alt="test" />
                        </div>
                    </div>
                </div>
            </div>
            <div style="border:1px solid #808080;width:406px;height:78px;margin-left:5px">
                <div class="row" style="margin-top:0px;width:500px;">
                    <div class="col-1" style="margin-left:10px"><label>PRO</label></div>
                    <div class="col-5" style="margin-right:40px">
                        <input name="PROname" id="PROname" style="cursor :default;height:25px;width:195px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px" class="form-control" />
                    </div>
                    <input name="PROKey" id="PROKey" style="cursor:default;height:25px;width:195px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px;display:none" class="form-control" />

                    <div class="col-6" style="margin-right:0px">
                        <input type="radio" id="PROnameCheck" style="margin-top:8px;" name="PROCheck" value="DATE" checked />

                        <label for="usr" style="margin-top:1px;font-family:Tahoma,Arial;margin-right:0px">&nbsp; Name&nbsp;&nbsp;&nbsp;</label>

                        <input type="radio" id="PROcodeCheck" style="margin-top:8px;" name="PROCheck" value="BILL" />
                        <label for="code" style="margin-top:1px;font-family:Tahoma,Arial;">&nbsp; Code</label>
                    </div>
                </div>

                <div class="row" style="margin-top:0px;width:500px;">
                    <div class="col-lg-1" style="margin-right:0px"><label>Catagory</label></div>
                    <div class="col-5" style="margin-right:40px">
                        <input name="catgName" id="catgName" style="cursor :default;height:25px;width:195px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px" class="form-control" />
                    </div>
                    <input name="catgKey" id="catgKey" style="cursor:default;height:25px;width:195px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px;display:none" class="form-control" />

                    <div class="col-6" style="margin-right:0px">
                        <input type="radio" id="catgNameCheck" style="margin-top:8px;" name="catgCheck" value="DATE" checked />

                        <label for="usr" style="margin-top:1px;font-family:Tahoma,Arial;margin-right:0px">&nbsp; Catagory&nbsp;&nbsp;&nbsp;</label>

                        <input type="radio" id="catgCodeCheck" style="margin-top:8px;" name="catgCheck" value="BILL" />
                        <label for="code" style="margin-top:1px;font-family:Tahoma,Arial;">&nbsp; Key</label>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
