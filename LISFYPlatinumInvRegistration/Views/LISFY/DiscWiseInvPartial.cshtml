@model LISFYPlatinumInvRegistration.Models.branchdetails
<div class="modal-dialog modal-lg textEditor " style="">

    <div class="modal-content box inner-outline" style="width:525px;margin-left:200px;margin-top:80px;border-color: #808080;">
        <div class="modal-body modal-popup " style="height:385px;width:525px;">

            <button type="button" class="close" id="discWiseInvClose" data-dismiss="modal" aria-label="Close">
                <i class="fas fa-times"></i>
            </button>
            @*justify-content-center*@

            <div class="row  buttons-header " style="border:0px solid #808080;width:656px;margin-left:5px;height:30px">


                <div class="col-lg-4 col-md-4 " style="margin-left:0px;margin-top:3px">
                    <label style="margin-top:2px;">From</label>
                    <input name="" id="discInvFrm" type="datetime" style="cursor :default;height:25px;width:100px;margin-top:3px;font-family:Tahoma,Arial;font-size:13px" class="form-control" value="@ViewBag.FROMDATE" readonly>

                    <input type="datetime" style="display:none;margin-top:10px" id="discInvFrmClndr" alt="tests" />

                </div>
                <div class="col-lg-4 col-md-4" style="margin-left:0px;margin-right:0px;margin-top:3px">
                    <label style="margin-top:2px;">To</label>
                    <input name="" id="discInvTo" type="datetime" style="cursor :default;height:25px;width:100px;margin-top:3px;font-family:Tahoma,Arial;font-size:13px" class="form-control" value="@ViewBag.TODATE" readonly>

                    <input type="datetime" style="display:none;margin-top:10px" id="discInvToClndr" alt="test" />

                </div>


                <div class="col-lg-2 col-md-3" style="margin-top:5px;font-family:Tahoma,Arial">
                    <button type="button" class="green-btn" value="btnDiscInv" id="btnDiscInv" style="height:27px;width:70px;margin-right:10px;;font-family:Verdana;font-size:13px">
                        Print
                    </button>
                </div>


            </div>
            <div class="row buttons-header " style="width:656px;margin-left:5px;height:40px">


                <div class="col-lg-7 col-md-4 col-sm-4" style="margin-left:0px;">
                    <div class="row" style="margin-top:10px">
                        <div class="col-1" style="margin-right:10px"><label>Branch</label></div>
                        <div class="col-8" style="margin-right:0px">
                            @*<input name="passport" id="passport" type="text" class="form-control" style="width:350px;height:25px">*@
                            <span>
                                @Html.DropDownListFor(model => model.BrMst_Key, new SelectList(ViewBag.brlist, "BrMst_Key", "BrMst_Name"), "<-- ALL -->",@*new { style = "width: 250px;" }) ,*@ new { @id = "discWiseBrid", @class = "form-control", placeholder = "FinancialYear", style = "width:315px;height:28px" })

                                @Html.ValidationMessageFor(model => model.BrMst_Key, "", new { @class = "text-danger" })
                            </span>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-4" style="margin-left:0px;margin-right:0px;margin-top:5px">
                    <input type="checkbox" id="mstrparent" style="margin-top:11px" /><label style="margin-top:4px;">  Select All</label>
                    @*<input type="checkbox" id="parent" style="margin-top:11px" />*@
                </div>

            </div>

            <div class="row" style="width:476px;margin-left:9px;height:50px">
                <table class="table table-striped table-bordered" style="overflow-y:scroll" id="BrnchWiseClctn">
                    <thead id="brchHead">

                        <tr>

                            <th scope="col" width="10%" style="font-family:Verdana;font-size:11px;color:white">SlNo</th>
                            <th scope="col" width="10%" style="font-family:Verdana;font-size:11px;color:white">Id</th>
                            <th scope="col" width="10%" style="font-family:Verdana;font-size:11px;color:white">Mark</th>
                            <th scope="col" width="70%" style="font-family:Verdana;font-size:11px;color:white">Description</th>

                        </tr>
                    </thead>
                    <tbody id="brchBody">

                        @if (ViewBag.mstrist != null)
                        {
                            var slno = 1;
                            foreach (var brnch in ViewBag.mstrist)
                            {

                                <tr class="item" id="testRows">
                                    <td id="selno" style="font-family:Verdana;font-size:12px;width:37px">@slno</td>
                                    <td id="Mstr_Key" style="font-family:Verdana;font-size:12px;width:37px">
                                        @brnch.Id
                                    </td>
                                    <td id="Mstr_Mark" style="font-family:Verdana;font-size:12px;width:37px">
                                        <center><input type="checkbox" id="mstrChk" name="mstrChk" class="mstrchild" style="margin-left:15px; margin-right:auto;" /> </center>

                                    </td>
                                    <td id="Mstr_Desc" style="font-family:Verdana;font-size:12px;width:260px">
                                        @brnch.Name
                                    </td>

                                </tr>
                                slno++;

                            }
                        }
                    </tbody>
                </table>


            </div>



        </div>
    </div>
</div>


