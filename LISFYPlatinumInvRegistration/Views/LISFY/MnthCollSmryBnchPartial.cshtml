<div class="modal-dialog modal-lg textEditor " style="">

    <div class="modal-content box inner-outline" style="width:700px;margin-left:200px;margin-top:80px;border-color: #808080;">
        <div class="modal-body modal-popup " style="height:345px;width:700px;">

            <button type="button" class="close" id="MonthlyBrnchClose" data-dismiss="modal" aria-label="Close">
                <i class="fas fa-times"></i>
            </button>


            <div class="row justify-content-center buttons-header " style="border:1px solid #808080;width:656px;margin-left:5px;height:40px">


                <div class="col-lg-4 col-md-4 " style="margin-left:0px;margin-top:3px">
                    <label style="margin-top:2px;">From</label>
                    <input name="" id="FrmMnthBrnch" type="datetime" style="cursor :default;height:25px;width:100px;margin-top:3px;font-family:Tahoma,Arial;font-size:13px" class="form-control" value="@ViewBag.FROMDATE" readonly>

                    <input type="datetime" style="display:none;margin-top:10px" id="FrmMnthBrnchClndr" alt="tests" />

                </div>
                <div class="col-lg-4 col-md-4" style="margin-left:0px;margin-right:0px;margin-top:3px">
                    <label style="margin-top:2px;">To</label>
                    <input name="" id="ToMnthBrnch" type="datetime" style="cursor :default;height:25px;width:100px;margin-top:3px;font-family:Tahoma,Arial;font-size:13px" class="form-control" value="@ViewBag.TODATE" readonly>

                    <input type="datetime" style="display:none;margin-top:10px" id="ToMnthBrnchClndr" alt="test" />

                </div>
                <div class="col-lg-3 col-md-4" style="margin-left:0px;margin-right:0px;margin-top:0px">
                    <input type="checkbox" id="parentchecks" style="margin-top:11px" /><label style="margin-top:4px;">  Select All</label>
                    @*<input type="checkbox" id="parent" style="margin-top:11px" />*@
                </div>

                <div class="col-lg-2 col-md-3" style="margin-top:5px;font-family:Tahoma,Arial">
                    <button type="button" class="green-btn" value="btnMnthCollBchWise" id="btnMnthCollBchWise" style="height:27px;width:60px;margin-right:10px;;font-family:Verdana;font-size:13px">
                        Print
                    </button>
                </div>


            </div>
            <div class="row" style="width:650px;margin-left:9px;height:50px">
                <table class="table table-striped table-bordered" style="overflow-y:scroll" id="BrnchWiseClctn">
                    <thead id="brchHead">

                        <tr>

                            <th scope="col" width="10%" style="font-family:Verdana;font-size:11px;color:white">SlNo</th>
                            <th scope="col" width="10%" style="font-family:Verdana;font-size:11px;color:white;display:none">Id</th>
                            <th scope="col" width="10%" style="font-family:Verdana;font-size:11px;color:white">Mark</th>
                            <th scope="col" width="70%" style="font-family:Verdana;font-size:11px;color:white">Description</th>

                        </tr>
                    </thead>
                    <tbody id="brchBody">

                        @if (ViewBag.brlist != null)
                        {
                            var slno = 1;
                            foreach (var brnch in ViewBag.brlist)
                            {

                                <tr class="item" id="testRows">
                                    <td id="Slno" style="font-family:Verdana;font-size:12px;width:37px">@slno</td>
                                    <td id="brid" style="font-family:Verdana;font-size:12px;width:37px;display:none">
                                        @brnch.BrMst_Key
                                    </td>
                                    <td id="mark" style="font-family:Verdana;font-size:12px;width:37px">
                                        <center><input type="checkbox" id="branchChk" name="branchChk" class="childchecks" style="margin-left:15px; margin-right:auto;" /> </center>

                                    </td>
                                    <td id="brname" style="font-family:Verdana;font-size:12px;width:260px">
                                        @brnch.BrMst_Name
                                    </td>

                                </tr>
                                slno++;

                            }
                        }
                    </tbody>
                </table>


            </div>



        </div>
    </div>
</div>

