@model LISFYPlatinumInvRegistration.Models.branchdetails
<div class="modal-dialog modal-lg textEditor " style="">

    <div class="modal-content box inner-outline" style="width:450px;margin-left:300px;margin-top:80px;border-color: #808080;">
        @*<div class="modal-header" style="background-color:#85b9e0;margin-top:8px;margin-left:30px;">

                <h6 class="modal-title" style="margin-top:8px;margin-left:30px;background-color:#85b9e0;color:black">Daily Collection Statement</h6>
            </div>*@
        <div class="modal-body modal-popup " style="height:260px;width:450px;">

            <button type="button" class="close" id="divSmryClose" data-dismiss="modal" aria-label="Close">
                <i class="fas fa-times"></i>
            </button>
            <div class="row justify-content-center buttons-header" style="border:1px solid #808080;width:406px;margin-left:5px;height:60px;background-color:#357EC7">


                <div class="col-lg-10 col-md-4 col-sm-4">
                    <div class="row" style="margin-top:20px">
                        @*<div class="col-1" style="margin-left:95px"><label>Branch</label></div>*@
                        <div class="col-8" style="margin-right:0px">
                            @*<input name="passport" id="passport" type="text" class="form-control" style="width:350px;height:25px">*@
                            <h5 style="color:white;width:350px">Division Collection Summary</h5>
                        </div>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3" style="margin-top:20px;font-family:Tahoma,Arial;margin-right:0px;">
                    <button type="button" class="green-btn" value="Refresh" id="btnMonDivCollSmry" style="height:27px;width:60px;margin-right:10px;font-family:Verdana;font-size:13px">
                        @*<label style="font-family:Verdana;font-size:13px;font-weight:bold;align-content:center">*@Print@*</label>*@
                    </button>

                </div>

            </div>
            <div class="row justify-content-center buttons-header " style="width:456px;margin-left:5px;height:40px">
                <div class="col-lg-12 col-md-4 " style="margin-left:90px">

                    <input type="radio" id="dailyChck" style="margin-top:8px;" name="dateChck" value="LABNO" checked />

                    <label for="dailyChck" style="margin-top:1px;font-family:Tahoma,Arial;">&nbsp; Daily</label>
                    <input type="radio" id="monthlyChck" style="margin-top:8px;" name="dateChck" value="NAME" />
                    <label for="monthlyChck" style="margin-top:1px;font-family:Tahoma,Arial;">&nbsp; Monthly</label>

                </div>
            </div>
            <div style="border:1px solid #808080;width:406px;height:108px;margin-left:5px">
                <div class="row  buttons-header " style="width:406px;margin-left:5px;height:40px;margin-top:15px;">
                    <div class="col-lg-10 col-md-4 col-sm-4">
                        <div class="row" style="margin-top:0px">
                            <div class="col-1" style="margin-right:10px"><label>Branch</label></div>
                            <div class="col-8" style="margin-right:0px">
                                @*<input name="passport" id="passport" type="text" class="form-control" style="width:350px;height:25px">*@
                                <span>
                                    @Html.DropDownListFor(model => model.BrMst_Key, new SelectList(ViewBag.brlist, "BrMst_Key", "BrMst_Name"),@*new { style = "width: 250px;" }) ,*@ new { @id = "brnchids", @class = "form-control", placeholder = "FinancialYear", style = "width: 315px;height:28px" })

                                    @Html.ValidationMessageFor(model => model.BrMst_Key, "", new { @class = "text-danger" })
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row  buttons-header " style="width:406px;margin-left:10px;height:40px;margin-top:10px">


                    <div class="col-lg-12 col-md-4 col-sm-4">
                        <div class="row" style="margin-top:0px">
                            <div class="col-1" style="margin-top:5px"><label>From</label></div>
                            <div class="col-5" style="margin-right:0px">
                                <input name="monDivFrm" id="monDivFrm" style="cursor :default;height:25px;width:100px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px" class="form-control" value="@ViewBag.FROMDATE" readonly />

                                <input type="" style="display:none;margin-top:10px" id="monDivFrmClndr" alt="tests" />
                            </div>
                            <div class="col-1" style="margin-top:5px"><label>To</label></div>
                            <div class="col-5" style="margin-right:0px">
                                <input name="monDivTo" id="monDivTo" style="cursor :default;height:25px;width:100px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px" class="form-control" value="@ViewBag.TODATE" readonly />

                                <input type="" style="display:none;margin-top:10px" id="monDivToClndr" alt="test" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
