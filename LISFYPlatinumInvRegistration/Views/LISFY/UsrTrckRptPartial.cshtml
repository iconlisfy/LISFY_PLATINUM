@model LISFYPlatinumInvRegistration.Models.branchdetails
<div class="modal-dialog modal-lg textEditor " style="">

    <div class="modal-content box inner-outline" style="width:500px;margin-left:300px;margin-top:80px;border-color: #808080;">
        @*<div class="modal-header" style="background-color:#85b9e0;margin-top:8px;margin-left:30px;">

                <h6 class="modal-title" style="margin-top:8px;margin-left:30px;background-color:#85b9e0;color:black">Daily Collection Statement</h6>
            </div>*@
        <div class="modal-body modal-popup " style="height:235px;width:500px;">

            <button type="button" class="close" id="usrTackClose" data-dismiss="modal" aria-label="Close">
                <i class="fas fa-times"></i>
            </button>
            <div class="row justify-content-center buttons-header" style="border:1px solid #808080;width:456px;margin-left:5px;height:60px;background-color:#357EC7">


                <div class="col-lg-10 col-md-4 col-sm-4">
                    <div class="row" style="margin-top:20px">
                        @*<div class="col-1" style="margin-left:95px"><label>Branch</label></div>*@
                        <div class="col-8" style="margin-right:0px">
                            @*<input name="passport" id="passport" type="text" class="form-control" style="width:350px;height:25px">*@
                            <h5 style="color:white;width:350px">User Track Report</h5>
                        </div>
                    </div>
                </div>
                <div class="col-lg-2 col-md-3" style="margin-top:20px;font-family:Tahoma,Arial">
                    <button type="button" class="green-btn" value="Refresh" id="btnusrTrackRpt" style="height:27px;width:60px;margin-right:10px;font-family:Verdana;font-size:13px">
                        @*<label style="font-family:Verdana;font-size:13px;font-weight:bold;align-content:center">*@Print@*</label>*@
                    </button>

                </div>

            </div>
            <div class="row buttons-header " style="width:456px;margin-left:10px;height:30px">

                <div class="col-lg-5 col-md-4 " style="margin-right:20px;margin-top:2px">
                    <label style="margin-top:8px;">From</label>
                    <input name="usrTrckFrm" id="usrTrckFrm" @*type="datetime"*@ style="cursor :default;height:25px;width:100px;margin-top:8px;font-family:Tahoma,Arial;font-size:13px" class="form-control" value="@ViewBag.FROMDATE" readonly>

                    <input type="" style="display:none;margin-top:10px" id="usrTrckFrmClndr" alt="tests" />

                </div>
                <div class="col-lg-5 col-md-4" style="margin-left:0px;margin-right:0px;margin-top:2px">
                    <label style="margin-top:8px;">To</label>
                    <input name="usrTrckTo" id="usrTrckTo" @*type="datetime"*@ style="cursor :default;height:25px;width:100px;margin-top:8px;font-family:Tahoma,Arial;font-size:13px" class="form-control" value="@ViewBag.TODATE" readonly>

                    <input type="" style="display:none;margin-top:10px" id="usrTrckToClndr" alt="test" />

                </div>



            </div>
            <div class="row buttons-header " style="width:456px;margin-left:5px;height:40px">


                <div class="col-lg-10 col-md-4 col-sm-4">
                    <div class="row" style="margin-top:10px">
                        <div class="col-1" style="margin-left:5px"><label>Branch</label></div>
                        <div class="col-8" style="margin-left:12px">
                            @*<input name="passport" id="passport" type="text" class="form-control" style="width:350px;height:25px">*@
                            <span>
                                @*BrMstKey BrMstName*@
                                @Html.DropDownListFor(model => model.BrMst_Key, new SelectList(ViewBag.brlist, "BrMst_Key", "BrMst_Name"), "<-- ALL -->",@*new { style = "width: 250px;" }) ,*@ new { @id = "usrTrckBrnchKey", @class = "form-control", placeholder = "FinancialYear", style = "width: 315px;height:28px" })

                                @Html.ValidationMessageFor(model => model.BrMst_Key, "", new { @class = "text-danger" })
                            </span>
                        </div>
                    </div>
                </div>


            </div>
            <div class="row buttons-header " style="border:0px solid #808080;width:456px;margin-left:5px;height:25px">
                <div class="col-lg-3" style="margin-top:10px;font-size:12px">User Name</div>
                <div class="col-5" style="margin-right:57px">
                    <input name="usrNames" id="usrNames" style="cursor :default;height:25px;width:195px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px" class="form-control" />
                </div>
                <input name="UsrKeys" id="UsrKeys" style="cursor:default;height:25px;width:195px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px;display:none" class="form-control" />

                <div class="col-5" style="margin-top:2px">
                    <input type="radio" id="usrNameCheck" style="margin-top:8px;" name="userNameCodeCheck" value="DATE" checked />

                    <label for="usr" style="margin-top:1px;font-family:Tahoma,Arial;margin-right:0px">&nbsp; Name&nbsp;&nbsp;&nbsp;</label>

                    <input type="radio" id="usrCodesCheck" style="margin-top:8px;" name="userNameCodeCheck" value="BILL" />
                    <label for="code" style="margin-top:1px;font-family:Tahoma,Arial;">&nbsp; Code</label>
                </div>


            </div>
        </div>
    </div>
</div>
