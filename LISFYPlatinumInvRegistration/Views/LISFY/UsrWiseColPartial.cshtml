@model LISFYPlatinumInvRegistration.Models.branchdetails
<div class="modal-dialog modal-lg textEditor " style="">

    <div class="modal-content box inner-outline" style="width:500px;margin-left:300px;margin-top:80px;border-color: #808080;">
        @*<div class="modal-header" style="background-color:#85b9e0;margin-top:8px;margin-left:30px;">

                <h6 class="modal-title" style="margin-top:8px;margin-left:30px;background-color:#85b9e0;color:black">Daily Collection Statement</h6>
            </div>*@
        <div class="modal-body modal-popup " style="height:300px;width:500px;">

            <button type="button" class="close" id="usrWiseClose" data-dismiss="modal" aria-label="Close">
                <i class="fas fa-times"></i>
            </button>


            <form id="newForm">
                <div class="row justify-content-center buttons-header" style="border:1px solid #808080;width:456px;margin-left:5px;height:60px;background-color:#357EC7">


                    <div class="col-lg-8 col-md-4 col-sm-4">
                        <div class="row" style="margin-top:20px">
                            @*<div class="col-1" style="margin-left:95px"><label>Branch</label></div>*@
                            <div class="col-8" style="margin-right:0px">
                                @*<input name="passport" id="passport" type="text" class="form-control" style="width:350px;height:25px">*@
                                <h5 style="color:white;width:350px">User Wise Collection</h5>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-2 col-md-3" style="margin-top:20px;font-family:Tahoma,Arial">
                        <button type="button" class="green-btn" value="Refresh" id="rfrshUsrWiseClctn" style="height:27px;width:60px;margin-right:20px;font-family:Verdana;font-size:13px">
                            New
                        </button>

                    </div>
                    <div class="col-lg-2 col-md-3" style="margin-top:20px;font-family:Tahoma,Arial">
                        <button type="button" class="green-btn" value="Refresh" id="btnUsrWiseClctn" style="height:27px;width:60px;margin-right:10px;font-family:Verdana;font-size:13px">
                            @*<label style="font-family:Verdana;font-size:13px;font-weight:bold;align-content:center">*@Print@*</label>*@
                        </button>

                    </div>

                </div>
                <div class="row justify-content-center buttons-header " style="width:456px;margin-left:5px;height:60px">
                    <div class="row  buttons-header " style="width:456px;margin-left:5px;height:40px;margin-top:5px;">
                        <div class="col-lg-10 col-md-4 col-sm-4">
                            <div class="row" style="margin-top:0px">
                                <div class="col-1" style="margin-right:10px"><label>Branch</label></div>
                                <div class="col-8" style="margin-right:0px">
                                    @*<input name="passport" id="passport" type="text" class="form-control" style="width:350px;height:25px">*@
                                    <span>
                                        @Html.DropDownListFor(model => model.BrMst_Key, new SelectList(ViewBag.brlist, "BrMst_Key", "BrMst_Name"), "<--All-->",@*new { style = "width: 250px;" }) ,*@ new { @id = "bnchKey", @class = "form-control", placeholder = "FinancialYear", style = "width: 315px;height:28px" })

                                        @Html.ValidationMessageFor(model => model.BrMst_Key, "", new { @class = "text-danger" })
                                    </span>

                                </div>
                            </div>
                            <div class="row" style="margin-top:0px;width:500px;">
                                <div class="col-1" style="margin-right:0px"><label>User</label></div>
                                <div class="col-5" style="margin-right:40px">
                                    <input name="usrName" id="usrName" style="cursor :default;height:25px;width:195px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px" class="form-control" />
                                </div>
                                <input name="usrKey" id="usrKey" style="cursor:default;height:25px;width:195px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px;display:none" class="form-control" />

                                <div class="col-6" style="margin-right:0px">
                                    <input type="radio" id="usrCheck" style="margin-top:8px;" name="usrCodeCheck" value="DATE" checked />

                                    <label for="usr" style="margin-top:1px;font-family:Tahoma,Arial;margin-right:0px">&nbsp; User&nbsp;&nbsp;&nbsp;</label>

                                    <input type="radio" id="codeCheck" style="margin-top:8px;" name="usrCodeCheck" value="BILL" />
                                    <label for="code" style="margin-top:1px;font-family:Tahoma,Arial;">&nbsp; Code</label>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <div class="row buttons-header " style="width:456px;margin-left:25px;height:30px">
                    <div class="row" style="margin-top:0px;width:420px">


                        <input type="radio" id="dateCheck" style="margin-top:8px;" name="usrCheck" value="DATE" checked />

                        <label for="dateWiseCheck" style="margin-top:1px;font-family:Tahoma,Arial;margin-right:190px">&nbsp; Date Wise</label>
                        <input type="radio" id="billCheck" style="margin-top:8px;" name="usrCheck" value="BILL" />
                        <label for="billWiseCheck" style="margin-top:1px;font-family:Tahoma,Arial;">&nbsp; Bill No Wise</label>

                    </div>
                </div>
                <div class="row justify-content-center buttons-header " style="width:456px;margin-left:5px;height:60px">

                    <div class="col-lg-6 col-md-4 col-sm-4" id="dateField" style="border:.5px solid #808080;width:0px;height:93px">
                        <div class="row" style="margin-top:10px">
                            <div class="col-1" style="margin-top:5px"><label>From</label></div>
                            <div class="col-10" style="margin-left:18px">
                                <input name="usrWiseFrm" id="usrWiseFrm" style="cursor :default;height:25px;width:100px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px" class="form-control" value="@ViewBag.FROMDATE" readonly />

                                <input type="" style="display:none;margin-top:10px" id="usrWiseFrmClndr" alt="tests" />
                            </div>
                            <div class="col-1" style="margin-top:5px"><label>To</label></div>
                            <div class="col-8" style="margin-left:18px">
                                <input name="usrWiseTo" id="usrWiseTo" style="cursor :default;height:25px;width:100px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px" class="form-control" value="@ViewBag.TODATE" readonly />

                                <input type="" style="display:none;margin-top:10px" id="usrWiseToClndr" alt="test" />
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-3" style="border:.5px solid #808080;width:0px;height:93px">
                        <div class="row" style="margin-top:10px">
                            <div class="col-1" style="margin-top:5px"><label>From</label></div>
                            <div class="col-10" style="margin-left:18px">
                                <input name="usrWsFrm" id="usrWsFrm" autocomplete="off" style="cursor :default;height:25px;width:160px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px" class="form-control" />

                                @*<input type="" style="display:none;margin-top:10px" id="usrWiseFrmClndr" alt="tests" />*@
                            </div>
                            <div class="col-1" style="margin-top:5px"><label>To</label></div>
                            <div class="col-8" style="margin-left:18px">
                                <input name="usrWsTo" id="usrWsTo" autocomplete="off" style="cursor :default;height:25px;width:160px;margin-top:5px;font-family:Tahoma,Arial;font-size:13px" class="form-control" />

                                @*<input type="" style="display:none;margin-top:10px" id="usrWiseToClndr" alt="test" />*@
                            </div>
                        </div>

                    </div>

                </div>
            </form>

        </div>
    </div>
</div>
