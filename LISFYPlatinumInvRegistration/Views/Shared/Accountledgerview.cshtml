    *@
    <div class="modal-dialog modal-popup" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Account Ledger</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" id="closeaccledger" style="background-color:#428bca">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-bg-div">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="field-item">
                                <label for="">Ledger Name</label>
                                <input type="text" id="Ahmst_key" style="display:none" required="" class="fcs">
                                <input type="text" id="ledgername" name="ledgername" class="bold-input-field fcs" onkeyup="if ( event.keyCode == 27 ) this.value=''" >
                                @*<input type="text" class="form-control" id="ledgername">*@
                                <button class="searchbtn" data-toggle="modal" data-target="#searchlookup" data-backdrop="static" data-keyboard="false" type="button" id="accledg"><i class="fas fa-search"></i></button>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <div class="field-item">
                                <label for="">A/c group Name</label>
                                <input type="text" id="acgrpname" onkeyup="if ( event.keyCode == 27 ) this.value=''" class="fcs" readonly>
                                <input type="text" id="acgrpid" style="display:none">
                                <button class="searchbtn" data-toggle="modal" data-target="#searchlookup" data-backdrop="static" data-keyboard="false" type="button" id="accgrp"><i class="fas fa-search"></i></button>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <div class="field-item">
                                <label for="">Group</label>
                                <input type="text" id="group" onkeyup="if ( event.keyCode == 27 ) this.value=''" class="fcs" readonly>
